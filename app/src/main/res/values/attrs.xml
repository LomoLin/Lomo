<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 图片展示控件 -->
    <declare-styleable name="ImageGallery">
        <attr name="indicatorUnSelected" format="reference" />
        <attr name="indicatorSelected" format="reference" />
        <attr name="showIndicator" format="boolean" />
    </declare-styleable>
    <!-- 时间轴 -->
    <declare-styleable name="TimeLineMarker">
        <attr name="markerSize" format="dimension" />
        <attr name="marker" format="color|reference" />
        <attr name="beginLine" format="color|reference" />
        <attr name="endLine" format="color|reference" />
        <attr name="lineSize" format="dimension" />
    </declare-styleable>
</resources>